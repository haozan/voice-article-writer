# Override Clacky platform environment variables with multi-LLM provider settings
# Copy this file to .env and fill in your actual API keys

# Grok (X AI) - Default LLM provider
CLACKY_LLM_BASE_URL=https://api.x.ai/v1
CLACKY_LLM_API_KEY=your_grok_api_key_here
CLACKY_LLM_MODEL=grok-4-1-fast-reasoning

# Qwen (Alibaba Cloud) - 千问
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL=qwen3-max

# DeepSeek - 深度推理模型
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-reasoner

# Gemini (Google) - Google AI
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=models/gemini-3-flash-preview

# Zhipu (智谱 AI) - GLM 模型
ZHIPU_BASE_URL=https://open.bigmodel.cn/api/paas/v4
ZHIPU_API_KEY=your_zhipu_api_key_here
ZHIPU_MODEL=glm-4-flash

# ChatGPT (OpenAI) - GPT 模型
CHATGPT_BASE_URL=https://api.openai.com/v1
CHATGPT_API_KEY=your_chatgpt_api_key_here
CHATGPT_MODEL=gpt-5.2
