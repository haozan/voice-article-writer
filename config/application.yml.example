# Gemini (Google) Configuration
GEMINI_BASE_URL_OPTIONAL: '<%= ENV.fetch("GEMINI_BASE_URL", "https://generativelanguage.googleapis.com/v1beta") %>'
GEMINI_API_KEY_OPTIONAL: '<%= ENV.fetch("GEMINI_API_KEY", "") %>'
GEMINI_MODEL_OPTIONAL: '<%= ENV.fetch("GEMINI_MODEL", "models/gemini-3-flash-preview") %>'
