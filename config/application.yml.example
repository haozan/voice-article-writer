DEEPSEEK_API_KEY_OPTIONAL: '<%= ENV.fetch("DEEPSEEK_API_KEY", "") %>'
DEEPSEEK_MODEL_OPTIONAL: '<%= ENV.fetch("DEEPSEEK_MODEL", "deepseek-reasoner") %>'
